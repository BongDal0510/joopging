<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.team4.joopging.mappers.ShopMapper">


    <select id="getGoodsList" resultType="shopVO">
        SELECT goodsNum, goodsName, goodsCategory, goodsPrice, goodsContent
        FROM TBL_GOODSLIST
        <![CDATA[
        WHERE goodsNum > 0
        ]]>
    </select>

</mapper>